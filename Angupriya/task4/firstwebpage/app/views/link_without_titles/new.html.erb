<h1>Creating Link  without title</h1>

<% form_for(@link) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_field :content %>
  </p>
   <p>
    <%= f.submit 'Create',:class => 'but'%>
  </p>
  <p><%=notice %></p>
<% end %>
