<h1>list of entries of records...........</h1>
  <table border='1'>
  <td>Name</td>
  <td>Id</td>

  <%#= raise @posts.inspect%>
  <tr ng-repeat="post in @posts"> 
  <%#= raise "hello inside index"%>
  <%#= Rails.logger.info "#{post.inspect}"%>
  <td>{{post.detail.id}}</td>
  <td>{{post.detail.name}}</td>
    <td><a ui-sref="edit({editId: {{post.detail.id}} })" >Edit</a></td>
    <td><a ui-sref="delete({deleteId: {{post.detail.id}} })" >Delete</a>
  </td>
  </tr>
</table>
